#include<stdio.h>
int main(){
    int a,b,c,d,p;
    printf("Please input four integer values\n");
    scanf("%d %d %d %d\n",&a,&b,&c,&d);
    printf("The input numbers are : %d %d %d %d\n",a,b,c,d);
    if(a>b)
    {
        if(a>c)
        {
            if(a>d)
            {
                p=a;
            }
            else
            {
                p=b;
            }
        }
        else
        {
            if(c>d)
            {
                p=c;
            }
            else
            {
                p=d;
            }
        }
    }
    else
    {
        if(b>c)
        {
            if(b>d)
            {
                p=b;
            }
            else
            {
                p=d;
            }
        }
        else
        {
            if(c>d)
            {
                p=c;
            }
            else
            {
                p=d;
            }
        }
    }
    printf("Maximum number among these four inputs is : %d\n",p);
    return 0;
}
